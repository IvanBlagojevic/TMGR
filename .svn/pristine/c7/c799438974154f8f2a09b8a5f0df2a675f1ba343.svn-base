package org.gs4tr.termmanager.model.dto;

import java.util.Map;

import org.gs4tr.termmanager.model.TermTooltip;

public class MultilingualTerm {
    private Long _availableTasks;

    private Boolean _inTranslation = Boolean.FALSE;

    private String _projectName;

    private String _projectTicket;

    private String _sourceAlignment;

    private Boolean _sourceInTranslation = Boolean.FALSE;

    private TargetDtoTermTranslation[] _sourceSynonyms;

    private TargetDtoTermTranslation _sourceTerm;

    private TermTooltip[] _sourceTermTooltip;

    private String _submissionTicket;

    private Map<String, TargetTerm> _targetTerms;

    private Long _termEntryCreationDate;

    private String _termEntryTicket;

    private String _ticket;

    private String _userCreated;

    public Long getAvailableTasks() {
	return _availableTasks;
    }

    public String getProjectName() {
	return _projectName;
    }

    public String getProjectTicket() {
	return _projectTicket;
    }

    public String getSourceAlignment() {
	return _sourceAlignment;
    }

    public TargetDtoTermTranslation[] getSourceSynonyms() {
	return _sourceSynonyms;
    }

    public TargetDtoTermTranslation getSourceTerm() {
	return _sourceTerm;
    }

    public TermTooltip[] getSourceTermTooltip() {
	return _sourceTermTooltip;
    }

    public String getSubmissionTicket() {
	return _submissionTicket;
    }

    public Map<String, TargetTerm> getTargetTerms() {
	return _targetTerms;
    }

    public Long getTermEntryCreationDate() {
	return _termEntryCreationDate;
    }

    public String getTermEntryTicket() {
	return _termEntryTicket;
    }

    public String getTicket() {
	return _ticket;
    }

    public String getUserCreated() {
	return _userCreated;
    }

    public Boolean isInTranslation() {
	return _inTranslation;
    }

    public Boolean isSourceInTranslation() {
	return _sourceInTranslation;
    }

    public void setAvailableTasks(Long availableTasks) {
	_availableTasks = availableTasks;
    }

    public void setInTranslation(Boolean inTranslation) {
	_inTranslation = inTranslation;
    }

    public void setProjectName(String projectName) {
	_projectName = projectName;
    }

    public void setProjectTicket(String projectTicket) {
	_projectTicket = projectTicket;
    }

    public void setSourceAlignment(String sourceAlignment) {
	_sourceAlignment = sourceAlignment;
    }

    public void setSourceInTranslation(Boolean sourceInTranslation) {
	_sourceInTranslation = sourceInTranslation;
    }

    public void setSourceSynonyms(TargetDtoTermTranslation[] sourceSynonyms) {
	_sourceSynonyms = sourceSynonyms;
    }

    public void setSourceTerm(TargetDtoTermTranslation sourceTerm) {
	_sourceTerm = sourceTerm;
    }

    public void setSourceTermTooltip(TermTooltip[] sourceTermTooltip) {
	_sourceTermTooltip = sourceTermTooltip;
    }

    public void setSubmissionTicket(String submissionTicket) {
	_submissionTicket = submissionTicket;
    }

    public void setTargetTerms(Map<String, TargetTerm> targetTerms) {
	_targetTerms = targetTerms;
    }

    public void setTermEntryCreationDate(Long termEntryCreationDate) {
	_termEntryCreationDate = termEntryCreationDate;
    }

    public void setTermEntryTicket(String termEntryTicket) {
	_termEntryTicket = termEntryTicket;
    }

    public void setTicket(String ticket) {
	_ticket = ticket;
    }

    public void setUserCreated(String userCreated) {
	_userCreated = userCreated;
    }
}
