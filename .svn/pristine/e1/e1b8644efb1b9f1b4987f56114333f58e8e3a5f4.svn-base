package org.gs4tr.termmanager.webmvc.model.response;

import java.util.HashMap;
import java.util.Map;

public class ProjectInfoModel {

    private Map<String, Boolean> _actionsAvailable;
    private boolean _addEditApproved;
    private boolean _addEditAttributes;
    private boolean _addEditBlacklist;
    private boolean _addEditOnHold;
    private boolean _addEditPending;
    private String _defaultTermStatus;
    private boolean _deleteAvailable;
    private String _projectTicket;

    public ProjectInfoModel() {
	_actionsAvailable = new HashMap<>();
    }

    public Map<String, Boolean> getActionsAvailable() {
	return _actionsAvailable;
    }

    public String getDefaultTermStatus() {
	return _defaultTermStatus;
    }

    public String getProjectTicket() {
	return _projectTicket;
    }

    public boolean isAddEditApproved() {
	return _addEditApproved;
    }

    public boolean isAddEditAttributes() {
	return _addEditAttributes;
    }

    public boolean isAddEditBlacklist() {
	return _addEditBlacklist;
    }

    public boolean isAddEditOnHold() {
	return _addEditOnHold;
    }

    public boolean isAddEditPending() {
	return _addEditPending;
    }

    public boolean isDeleteAvailable() {
	return _deleteAvailable;
    }

    public void setActionsAvailable(Map<String, Boolean> actionsAvailable) {
	this._actionsAvailable = actionsAvailable;
    }

    public void setAddEditApproved(boolean addEditApproved) {
	_addEditApproved = addEditApproved;
    }

    public void setAddEditAttributes(boolean addEditAttributes) {
	_addEditAttributes = addEditAttributes;
    }

    public void setAddEditBlacklist(boolean addEditBlacklist) {
	_addEditBlacklist = addEditBlacklist;
    }

    public void setAddEditOnHold(boolean addEditOnHold) {
	_addEditOnHold = addEditOnHold;
    }

    public void setAddEditPending(boolean addEditPending) {
	_addEditPending = addEditPending;
    }

    public void setDefaultTermStatus(String defaultTermStatus) {
	_defaultTermStatus = defaultTermStatus;
    }

    public void setDeleteAvailable(boolean deleteAvailable) {
	_deleteAvailable = deleteAvailable;
    }

    public void setProjectTicket(String projectTicket) {
	_projectTicket = projectTicket;
    }

}
