package org.gs4tr.termmanager.model.dto;

import com.thoughtworks.xstream.annotations.XStreamAlias;
import com.thoughtworks.xstream.annotations.XStreamOmitField;

public class Attribute {

    @XStreamAlias("attributeLevel")
    private AttributeLevelEnum _attributeLevelEnum;

    @XStreamAlias("attributeName")
    private String _attributeName;

    @XStreamAlias("attributeType")
    private TermEntryAttributeTypeDto _attributeType;

    @XStreamAlias("attributeValue")
    private String _attributeValue;

    @XStreamAlias("comboValues")
    private String _comboValues;

    @XStreamAlias("inputFieldType")
    private InputFieldTypeEnum _inputFieldTypeEnum;

    @XStreamAlias("isCustom")
    private Boolean _isCustom;

    @XStreamAlias("multiplicity")
    private Boolean _multiplicity;

    @XStreamAlias("readOnly")
    private Boolean _readOnly;

    @XStreamOmitField
    private String _renameValue;

    @XStreamAlias("required")
    private Boolean _required;

    @XStreamAlias("synchronizable")
    private Boolean _synchronizable;

    public AttributeLevelEnum getAttributeLevelEnum() {
	return _attributeLevelEnum;
    }

    public String getAttributeName() {
	return _attributeName;
    }

    public TermEntryAttributeTypeDto getAttributeType() {
	return _attributeType;
    }

    public String getAttributeValue() {
	return _attributeValue;
    }

    public String getComboValues() {
	return _comboValues;
    }

    public InputFieldTypeEnum getInputFieldTypeEnum() {
	return _inputFieldTypeEnum;
    }

    public Boolean getIsCustom() {
	return _isCustom;
    }

    public Boolean getMultiplicity() {
	return _multiplicity;
    }

    public Boolean getReadOnly() {
	return _readOnly;
    }

    public String getRenameValue() {
	return _renameValue;
    }

    public Boolean getRequired() {
	return _required;
    }

    public Boolean getSynchronizable() {
	return _synchronizable;
    }

    public void setAttributeLevelEnum(AttributeLevelEnum attributeLevelEnum) {
	_attributeLevelEnum = attributeLevelEnum;
    }

    public void setAttributeName(String attributeName) {
	_attributeName = attributeName;
    }

    public void setAttributeType(TermEntryAttributeTypeDto attributeType) {
	_attributeType = attributeType;
    }

    public void setAttributeValue(String attributeValue) {
	_attributeValue = attributeValue;
    }

    public void setComboValues(String comboValues) {
	_comboValues = comboValues;
    }

    public void setInputFieldTypeEnum(InputFieldTypeEnum inputFieldTypeEnum) {
	_inputFieldTypeEnum = inputFieldTypeEnum;
    }

    public void setIsCustom(Boolean isCustom) {
	_isCustom = isCustom;
    }

    public void setMultiplicity(Boolean multiplicity) {
	_multiplicity = multiplicity;
    }

    public void setReadOnly(Boolean readOnly) {
	_readOnly = readOnly;
    }

    public void setRenameValue(String renameValue) {
	_renameValue = renameValue;
    }

    public void setRequired(Boolean required) {
	_required = required;
    }

    public void setSynchronizable(Boolean synchronizable) {
	_synchronizable = synchronizable;
    }

}
