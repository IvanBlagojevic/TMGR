package org.gs4tr.termmanager.service.model.command;

import java.util.List;

public class TermEntryHistoryCommand {

    private String _termEntryId;
    private List<String> _gridLanguages;
    private boolean _showAllLanguages;

    public List<String> getGridLanguages() {
	return _gridLanguages;
    }

    public String getTermEntryId() {
	return _termEntryId;
    }

    public void setGridLanguages(List<String> gridLanguages) {
	_gridLanguages = gridLanguages;
    }

    public void setShowAllLanguages(boolean showAllLanguages) {
	_showAllLanguages = showAllLanguages;
    }

    public void setTermEntryId(String termEntryId) {
	_termEntryId = termEntryId;
    }

    public boolean showAllLanguages() {
	return _showAllLanguages;
    }
}
