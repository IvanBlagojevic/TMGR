package org.gs4tr.termmanager.model.dto;

import java.util.List;

public class ChildModel {

    private String _text;
    private String _xmlType;
    private String _value;
    private String _markerId;
    private String _parentMarkerId;
    private String _termTicket;
    private String _type;

    private List<ChildModel> _children;

    public List<ChildModel> getChildren() {
	return _children;
    }

    public String getMarkerId() {
	return _markerId;
    }

    public String getParentMarkerId() {
	return _parentMarkerId;
    }

    public String getTermTicket() {
	return _termTicket;
    }

    public String getText() {
	return _text;
    }

    public String getType() {
	return _type;
    }

    public String getValue() {
	return _value;
    }

    public String getXmlType() {
	return _xmlType;
    }

    public void setChildren(List<ChildModel> children) {
	_children = children;
    }

    public void setMarkerId(String markerId) {
	_markerId = markerId;
    }

    public void setParentMarkerId(String parentMarkerId) {
	_parentMarkerId = parentMarkerId;
    }

    public void setTermTicket(String termTicket) {
	_termTicket = termTicket;
    }

    public void setText(String text) {
	_text = text;
    }

    public void setType(String type) {
	_type = type;
    }

    public void setValue(String value) {
	_value = value;
    }

    public void setXmlType(String xmlType) {
	_xmlType = xmlType;
    }

}
