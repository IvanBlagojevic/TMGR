package org.gs4tr.termmanager.model;

import java.io.Serializable;
import java.util.List;

public class MultilingualTerm extends AbstractItemHolder {

    private static final long serialVersionUID = 7138243850793063684L;

    private String _projectName;

    private Boolean _searchedForbidden = Boolean.FALSE;

    private String _searchedTermName;

    private Boolean _showAutoSaved = Boolean.TRUE;

    private String _sourceLanguage;

    private Boolean _submitterView = Boolean.TRUE;

    private Long _termEntryCreationDate;

    private String _termEntryId;

    private List<TermHolder> _terms;

    private String _userCreated;

    @Override
    public Serializable getIdentifier() {
	return getTermEntryId();
    }

    public String getProjectName() {
	return _projectName;
    }

    public String getSearchedTermName() {
	return _searchedTermName;
    }

    public String getSourceLanguage() {
	return _sourceLanguage;
    }

    public Long getTermEntryCreationDate() {
	return _termEntryCreationDate;
    }

    public String getTermEntryId() {
	return _termEntryId;
    }

    public List<TermHolder> getTerms() {
	return _terms;
    }

    public String getUserCreated() {
	return _userCreated;
    }

    public Boolean isSearchedForbidden() {
	return _searchedForbidden;
    }

    public Boolean isShowAutoSaved() {
	return _showAutoSaved;
    }

    public Boolean isSubmitterView() {
	return _submitterView;
    }

    public void setProjectName(String projectName) {
	_projectName = projectName;
    }

    public void setSearchedForbidden(Boolean searchedForbidden) {
	_searchedForbidden = searchedForbidden;
    }

    public void setSearchedTermName(String searchedTermName) {
	_searchedTermName = searchedTermName;
    }

    public void setShowAutoSaved(Boolean showAutoSaved) {
	_showAutoSaved = showAutoSaved;
    }

    public void setSourceLanguage(String sourceLanguage) {
	_sourceLanguage = sourceLanguage;
    }

    public void setSubmitterView(Boolean submitterView) {
	_submitterView = submitterView;
    }

    public void setTermEntryCreationDate(Long termEntryCreationDate) {
	_termEntryCreationDate = termEntryCreationDate;
    }

    public void setTermEntryId(String termEntryId) {
	_termEntryId = termEntryId;
    }

    public void setTerms(List<TermHolder> terms) {
	_terms = terms;
    }

    public void setUserCreated(String userCreated) {
	_userCreated = userCreated;
    }

}
