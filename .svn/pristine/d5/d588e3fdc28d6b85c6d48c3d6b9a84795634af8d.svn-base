package org.gs4tr.termmanager.service.xls;

public class XlsConfiguration {

    public static final String TERM_ENTRY_ID = "TermEntryID";

    private int _maxColumn;

    private int _maxRow;

    private int _minColumn;

    private int _minRow;

    private int _rowIndex;

    public int getMaxColumn() {
	return _maxColumn;
    }

    public int getMaxRow() {
	return _maxRow;
    }

    public int getMinColumn() {
	return _minColumn;
    }

    public int getMinRow() {
	return _minRow;
    }

    public int getRowIndex() {
	return _rowIndex;
    }

    public void incrementMaxColumn() {
	_maxColumn++;
    }

    public void incrementRowIndex() {
	_rowIndex++;
    }

    public void setMaxColumn(int maxColumn) {
	_maxColumn = maxColumn;
    }

    public void setMaxRow(int maxRow) {
	_maxRow = maxRow;
    }

    public void setMinColumn(int minColumn) {
	_minColumn = minColumn;
    }

    public void setMinRow(int minRow) {
	_minRow = minRow;
    }

    public void setRowIndex(int rowIndex) {
	_rowIndex = rowIndex;
    }
}
