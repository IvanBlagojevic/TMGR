package org.gs4tr.termmanager.service.model.command;

import java.util.Set;

import org.gs4tr.foundation.modules.security.model.Policy;
import org.gs4tr.foundation.modules.security.model.RoleTypeEnum;

public class RoleCommand {

    private String _name;

    private Set<Policy> _policies;

    private String _roleId;

    private RoleTypeEnum _roleType;

    private Long _userId;

    public String getName() {
	return _name;
    }

    public Set<Policy> getPolicies() {
	return _policies;
    }

    public String getRoleId() {
	return _roleId;
    }

    public RoleTypeEnum getRoleType() {
	return _roleType;
    }

    public Long getUserId() {
	return _userId;
    }

    public void setName(String name) {
	_name = name;
    }

    public void setPolicies(Set<Policy> policies) {
	_policies = policies;
    }

    public void setRoleId(String roleId) {
	_roleId = roleId;
    }

    public void setRoleType(RoleTypeEnum roleType) {
	_roleType = roleType;
    }

    public void setUserId(Long userId) {
	_userId = userId;
    }

}