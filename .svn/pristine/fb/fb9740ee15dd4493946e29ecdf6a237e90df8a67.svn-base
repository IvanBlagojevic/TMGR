package org.gs4tr.termmanager.webmvc.model.commands;

public class EmailSchedulingCommand {

    private int _dailyHour = 0;

    private int _dayOfWeek = 0;

    private int _weeklyHour = 0;

    public int getDailyHour() {
	return _dailyHour;
    }

    public int getDayOfWeek() {
	return _dayOfWeek;
    }

    public int getWeeklyHour() {
	return _weeklyHour;
    }

    public void setDailyHour(int dailyHour) {
	_dailyHour = dailyHour;
    }

    public void setDayOfWeek(int dayOfWeek) {
	_dayOfWeek = dayOfWeek;
    }

    public void setWeeklyHour(int weeklyHour) {
	_weeklyHour = weeklyHour;
    }
}
