package org.gs4tr.termmanager.service.model.command;

import java.util.List;

public class DeleteTermCommands extends BaseTermActionCommand {

    private List<TermCommandPerProject> _deleteCommands;

    private boolean _deleteTermEntries = false;

    public List<TermCommandPerProject> getDeleteCommands() {
	return _deleteCommands;
    }

    public boolean isDeleteTermEntries() {
	return _deleteTermEntries;
    }

    public void setDeleteCommands(List<TermCommandPerProject> deleteCommands) {
	_deleteCommands = deleteCommands;
    }

    public void setDeleteTermEntries(boolean deleteTermEntries) {
	_deleteTermEntries = deleteTermEntries;
    }
}
