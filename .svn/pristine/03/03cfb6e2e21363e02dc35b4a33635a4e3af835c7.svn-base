<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.3.xsd
	http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.3.xsd">

	<context:component-scan
		base-package="org.gs4tr.foundation.modules.usermanager.service.impl" />
	<context:component-scan base-package="org.gs4tr.termmanager.service.listeners" />
	<context:component-scan
		base-package="org.gs4tr.foundation.modules.mail.service.impl" />
	<context:component-scan base-package="org.gs4tr.termmanager.service.impl" />
	<context:component-scan base-package="org.gs4tr.termmanager.service.utils" />
	<context:component-scan base-package="org.gs4tr.termmanager.service.mail.schedule" />
	<context:component-scan base-package="org.gs4tr.termmanager.service.lock.manager" />

	<bean id="passwordEncoder"
		class="org.springframework.security.authentication.encoding.ShaPasswordEncoder" />

	<bean id="importTermService"
		class="org.gs4tr.termmanager.service.impl.ImportTermServiceImpl" />

	<bean id="remoteGlossaryService" class="org.gs4tr.termmanager.tests.RemoteUserServiceImplTest" />

	<bean id="sessionContextAttributes" class="java.util.ArrayList">
		<constructor-arg>
			<list>
				<value>privateEncryptionAesKey</value>
			</list>
		</constructor-arg>
	</bean>

	<bean id="eventLoggingService" class="org.gs4tr.termmanager.tests.FakeLoggingService" />
</beans>
