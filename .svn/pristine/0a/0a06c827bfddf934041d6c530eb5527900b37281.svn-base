package org.gs4tr.termmanager.io.edd.api;

/**
 * A {@link Event} is an object with a specific type that is associated to a
 * specific {@link Handler}.
 */
public interface Event {

    /**
     * Returns the message type as a {@link Class} object. The message type is used
     * to handle events by their type.
     *
     * @return the message type as a {@link Class}.
     */
    Class<? extends Event> getType();
}
