package org.gs4tr.termmanager.service.model.command;

import java.util.List;

import org.gs4tr.termmanager.model.TranslationUnit;

public class SaveDashboardCommand {

    private Long _projectId;

    private boolean _saveTerm = false;

    private String _sourceLanguage;

    private List<String> _targetLanguage;

    private List<TranslationUnit> _translationUnits;

    public Long getProjectId() {
	return _projectId;
    }

    public String getSourceLanguage() {
	return _sourceLanguage;
    }

    public List<String> getTargetLanguage() {
	return _targetLanguage;
    }

    public List<TranslationUnit> getTranslationUnits() {
	return _translationUnits;
    }

    public boolean isSaveTerm() {
	return _saveTerm;
    }

    public void setProjectId(Long projectId) {
	_projectId = projectId;
    }

    public void setSaveTerm(boolean saveTerm) {
	_saveTerm = saveTerm;
    }

    public void setSourceLanguage(String sourceLanguage) {
	_sourceLanguage = sourceLanguage;
    }

    public void setTargetLanguage(List<String> targetLanguage) {
	_targetLanguage = targetLanguage;
    }

    public void setTranslationUnits(List<TranslationUnit> translationUnits) {
	_translationUnits = translationUnits;
    }

}
