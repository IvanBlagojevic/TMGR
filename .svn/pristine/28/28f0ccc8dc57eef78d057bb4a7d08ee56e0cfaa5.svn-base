package org.gs4tr.termmanager.service.model.command;

import java.util.List;
import java.util.Set;

public class SendToTranslationCommand extends BaseDashboardCommand {

    private Long _projectId;

    private boolean _reviewRequired = false;

    private String _sourceLanguage;

    private List<String> _sourceTermIds;

    private Long _submissionId;

    private String _submissionMarkerId;

    private String _submissionName;

    private Set<String> _termEntryIds;

    public Long getProjectId() {
	return _projectId;
    }

    public String getSourceLanguage() {
	return _sourceLanguage;
    }

    public List<String> getSourceTermIds() {
	return _sourceTermIds;
    }

    public Long getSubmissionId() {
	return _submissionId;
    }

    public String getSubmissionMarkerId() {
	return _submissionMarkerId;
    }

    public String getSubmissionName() {
	return _submissionName;
    }

    public Set<String> getTermEntryIds() {
	return _termEntryIds;
    }

    public boolean isReviewRequired() {
	return _reviewRequired;
    }

    public void setProjectId(Long projectId) {
	_projectId = projectId;
    }

    public void setReviewRequired(boolean reviewRequired) {
	_reviewRequired = reviewRequired;
    }

    public void setSourceLanguage(String sourceLanguage) {
	_sourceLanguage = sourceLanguage;
    }

    public void setSourceTermIds(List<String> sourceTermIds) {
	_sourceTermIds = sourceTermIds;
    }

    public void setSubmissionId(Long submissionId) {
	_submissionId = submissionId;
    }

    public void setSubmissionMarkerId(String submissionMarkerId) {
	_submissionMarkerId = submissionMarkerId;
    }

    public void setSubmissionName(String submissionName) {
	_submissionName = submissionName;
    }

    public void setTermEntryIds(Set<String> termEntryIds) {
	_termEntryIds = termEntryIds;
    }
}
