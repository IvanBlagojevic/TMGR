package org.gs4tr.termmanager.webmvc.model.search;

import java.util.List;

public class GridSearchBox extends SearchBox {

    private List<GridSearchItem> _items;

    private boolean _required = false;

    public GridSearchBox(SearchCriteria searchCriteria) {
	super(searchCriteria.getControlName());
    }

    public GridSearchBox() {
    }

    public List<GridSearchItem> getItems() {
	return _items;
    }

    public boolean isRequired() {
	return _required;
    }

    public void setItems(List<GridSearchItem> items) {
	_items = items;
    }

    public void setRequired(boolean required) {
	_required = required;
    }
}
