package org.gs4tr.termmanager.model;

import org.gs4tr.termmanager.model.search.ItemFolderEnum;

public class FolderPolicy {

    private final ItemFolderEnum _folder;

    private final String _policy;

    public FolderPolicy(ItemFolderEnum folder, String policy) {
	_folder = folder;
	_policy = policy;
    }

    public ItemFolderEnum getFolder() {
	return _folder;
    }

    public String getPolicy() {
	return _policy;
    }
}
