package org.gs4tr.termmanager.webmvc.model.response;

public class UiMenuItem {

    private UiDetailItem[] _detailsUrl;

    private String _id;

    private String _jsonSearch;

    private String _parent;

    private String[] _searchCriterias;

    private boolean _systemHidden = false;

    private String _url;

    public UiMenuItem(String id, String url, String parent, String jsonSearch, String[] searchCriterias,
	    boolean systemHidden) {
	_id = id;
	_url = url;
	_parent = parent;
	_jsonSearch = jsonSearch;
	_searchCriterias = searchCriterias;
	_systemHidden = systemHidden;
    }

    public UiMenuItem(String id, String url, String parent, String[] searchCriterias) {
	_id = id;
	_url = url;
	_searchCriterias = searchCriterias;
	_parent = parent;
    }

    public UiDetailItem[] getDetailsUrl() {
	return _detailsUrl;
    }

    public String getId() {
	return _id;
    }

    public String getJsonSearch() {
	return _jsonSearch;
    }

    public String getParent() {
	return _parent;
    }

    public String[] getSearchCriterias() {
	return _searchCriterias;
    }

    public String getUrl() {
	return _url;
    }

    public boolean isSystemHidden() {
	return _systemHidden;
    }

    public void setDetailsUrl(UiDetailItem[] detailsUrl) {
	_detailsUrl = detailsUrl;
    }

    public void setId(String id) {
	_id = id;
    }

    public void setJsonSearch(String jsonSearch) {
	_jsonSearch = jsonSearch;
    }

    public void setParent(String parent) {
	_parent = parent;
    }

    public void setSearchCriterias(String[] searchCriterias) {
	_searchCriterias = searchCriterias;
    }

    public void setSystemHidden(boolean systemHidden) {
	_systemHidden = systemHidden;
    }

    public void setUrl(String url) {
	_url = url;
    }
}
