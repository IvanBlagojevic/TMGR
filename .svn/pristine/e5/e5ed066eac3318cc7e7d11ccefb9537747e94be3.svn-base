package org.gs4tr.termmanager.model.dto;

import com.thoughtworks.xstream.annotations.XStreamAlias;

@XStreamAlias("termEntryLanguage")
public class TermEntryLanguage {

    @XStreamAlias("language")
    private String _language;

    @XStreamAlias("termEntry")
    private TermEntry _termEntry;

    @XStreamAlias("termEntryLanguageDescriptions")
    private Description[] _termEntryLanguageDescriptions;

    @XStreamAlias("terms")
    private Term[] _terms;

    @XStreamAlias("ticket")
    private String _ticket;

    public String getLanguage() {
	return _language;
    }

    public TermEntry getTermEntry() {
	return _termEntry;
    }

    public Description[] getTermEntryLanguageDescriptions() {
	return _termEntryLanguageDescriptions;
    }

    public Term[] getTerms() {
	return _terms;
    }

    public String getTicket() {
	return _ticket;
    }

    public void setLanguage(String language) {
	_language = language;
    }

    public void setTermEntry(TermEntry termEntry) {
	_termEntry = termEntry;
    }

    public void setTermEntryLanguageDescriptions(Description[] descriptions) {
	_termEntryLanguageDescriptions = descriptions;
    }

    public void setTerms(Term[] terms) {
	_terms = terms;
    }

    public void setTicket(String ticket) {
	_ticket = ticket;
    }

}