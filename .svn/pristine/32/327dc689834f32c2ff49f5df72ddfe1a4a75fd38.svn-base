package org.gs4tr.termmanager.model.search;

import java.util.Date;
import java.util.List;

public class AbstractSearchRequest extends ItemSearchRequest {

    private List<String> _creationUsers;

    private Date _dateCreatedFrom;

    private Date _dateCreatedTo;

    private Date _dateModifiedFrom;

    private Date _dateModifiedTo;

    private List<String> _hideBlanksLanguageIds;

    private boolean _missingTranslation = false;

    private List<String> _modificationUsers;

    private Long _projectId;

    private String _searchType;

    private boolean _sourceAndTargetSearch = true;

    private boolean _targetTermSearch = true;

    private String _term;

    private TypeSearchEnum _typeSearch = TypeSearchEnum.TERM;

    public List<String> getCreationUsers() {
	return _creationUsers;
    }

    public Date getDateCreatedFrom() {
	return _dateCreatedFrom;
    }

    public Date getDateCreatedTo() {
	return _dateCreatedTo;
    }

    public Date getDateModifiedFrom() {
	return _dateModifiedFrom;
    }

    public Date getDateModifiedTo() {
	return _dateModifiedTo;
    }

    public List<String> getHideBlanksLanguageIds() {
	return _hideBlanksLanguageIds;
    }

    public List<String> getModificationUsers() {
	return _modificationUsers;
    }

    public Long getProjectId() {
	return _projectId;
    }

    public String getSearchType() {
	return _searchType;
    }

    public String getTerm() {
	return _term;
    }

    public TypeSearchEnum getTypeSearch() {
	return _typeSearch;
    }

    public boolean isMissingTranslation() {
	return _missingTranslation;
    }

    public boolean isSourceAndTargetSearch() {
	return _sourceAndTargetSearch;
    }

    public boolean isTargetTermSearch() {
	return _targetTermSearch;
    }

    public void setCreationUsers(List<String> creationUsers) {
	_creationUsers = creationUsers;
    }

    public void setDateCreatedFrom(Date dateCreatedFrom) {
	_dateCreatedFrom = dateCreatedFrom;
    }

    public void setDateCreatedTo(Date dateCreatedTo) {
	_dateCreatedTo = dateCreatedTo;
    }

    public void setDateModifiedFrom(Date dateModifiedFrom) {
	_dateModifiedFrom = dateModifiedFrom;
    }

    public void setDateModifiedTo(Date dateModifiedTo) {
	_dateModifiedTo = dateModifiedTo;
    }

    public void setHideBlanksLanguageIds(List<String> hideBlanksLanguageIds) {
	_hideBlanksLanguageIds = hideBlanksLanguageIds;
    }

    public void setMissingTranslation(boolean missingTranslation) {
	_missingTranslation = missingTranslation;
    }

    public void setModificationUsers(List<String> modificationUsers) {
	_modificationUsers = modificationUsers;
    }

    public void setProjectId(Long projectId) {
	_projectId = projectId;
    }

    public void setSearchType(String searchType) {
	_searchType = searchType;
    }

    public void setSourceAndTargetSearch(boolean sourceAndTargetSearch) {
	_sourceAndTargetSearch = sourceAndTargetSearch;
    }

    public void setTargetTermSearch(boolean targetTermSearch) {
	_targetTermSearch = targetTermSearch;
    }

    public void setTerm(String term) {
	_term = term;
    }

    public void setTypeSearch(TypeSearchEnum typeSearch) {
	_typeSearch = typeSearch;
    }
}
