package org.gs4tr.termmanager.service.solr.restore.model;

public class RebuildException extends RuntimeException {

    /**
     * 
     */
    private static final long serialVersionUID = -1895627020687041358L;

    public RebuildException() {
	super();
    }

    public RebuildException(String message) {
	super(message);
    }

    public RebuildException(String message, Throwable throwable) {
	super(message, throwable);
    }

    public RebuildException(Throwable throwable) {
	super(throwable);
    }
}
