package org.gs4tr.termmanager.model.search.command;

import java.util.Date;
import java.util.Set;

import org.gs4tr.termmanager.model.TmUserProfile;
import org.gs4tr.termmanager.model.search.ItemFolderEnum;

public class UserProjectSearchRequest {

    private Date _dateModifiedFrom;

    private Date _dateModifiedTo;

    private ItemFolderEnum _folder;

    private Set<String> _languageIds;

    private String _name;

    private Set<Long> _projectIds;

    private String _shortCode;

    private TmUserProfile _user;

    private Long userId;

    public Date getDateModifiedFrom() {
	return _dateModifiedFrom;
    }

    public Date getDateModifiedTo() {
	return _dateModifiedTo;
    }

    public ItemFolderEnum getFolder() {
	return _folder;
    }

    public Set<String> getLanguageIds() {
	return _languageIds;
    }

    public String getName() {
	return _name;
    }

    public Set<Long> getProjectIds() {
	return _projectIds;
    }

    public String getShortCode() {
	return _shortCode;
    }

    public TmUserProfile getUser() {
	return _user;
    }

    public Long getUserId() {
	return userId;
    }

    public void setDateModifiedFrom(Date dateModifiedFrom) {
	_dateModifiedFrom = dateModifiedFrom;
    }

    public void setDateModifiedTo(Date dateModifiedTo) {
	_dateModifiedTo = dateModifiedTo;
    }

    public void setFolder(ItemFolderEnum folder) {
	_folder = folder;
    }

    public void setLanguageIds(Set<String> languageIds) {
	_languageIds = languageIds;
    }

    public void setName(String name) {
	_name = name;
    }

    public void setProjectIds(Set<Long> projectIds) {
	_projectIds = projectIds;
    }

    public void setShortCode(String shortCode) {
	_shortCode = shortCode;
    }

    public void setUser(TmUserProfile user) {
	_user = user;
    }

    public void setUserId(Long userId) {
	this.userId = userId;
    }
}
