package org.gs4tr.termmanager.webmvc.model.commands;

public class DownloadResourceCommand {

    private boolean _download = true;

    private String _downloadTicket;

    private boolean _sendWithoutDispositionHeader = false;

    public String getDownloadTicket() {
	return _downloadTicket;
    }

    public boolean isDownload() {
	return _download;
    }

    public boolean isSendWithoutDispositionHeader() {
	return _sendWithoutDispositionHeader;
    }

    public void setDownloadTicket(String downloadTicket) {
	_downloadTicket = downloadTicket;
    }

    public void setSendWithoutDispositionHeader(boolean sendWithoutDispositionHeader) {
	_sendWithoutDispositionHeader = sendWithoutDispositionHeader;
    }
}