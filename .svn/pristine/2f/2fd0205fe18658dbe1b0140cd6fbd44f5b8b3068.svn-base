package org.gs4tr.termmanager.webmvc.model.search;

public enum ControlType {

    CHECK_BOX("checkBox"),

    MULTI("multi"),

    TEXT("text"),

    LIST("list"),

    PLACEHOLDER("placeholder"),

    MULTI_AND_LIST("multiAndList"),

    MENU_CHECK("menuCheck"),

    TEXT_AND_LIST("textAndList"),

    DATE("date"),

    FROM_DATE("fromDate"),

    TO_DATE("toDate"),

    COMBO_CHECK_BOX("comboCheckBox"),

    COMBO_MULTI("comboMulti"),

    COMBO_MULTI_DOUBLE("comboMultiDouble"),

    COMBO_SINGLE("comboSingle"),

    COMBO_SINGLE_AND_COMBO_MULTI_LINKED("comboSingleAndComboMultiLinked"),

    DATE_RANGE("dateRange"),

    INPUT("input"),

    LINKED_COMBO("linkedCombo"),

    RADIO_BOX("radioBox"),

    TEXT_AND_COMBO_SINGLE("textAndComboSingle");

    private final String _command;

    private ControlType(String command) {
	_command = command;
    }

    public String getCommand() {
	return _command;
    }
}
